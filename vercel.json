{
  "buildCommand": "cd gsm-landing-page && npm run build",
  "outputDirectory": "gsm-landing-page/dist",
  "installCommand": "cd gsm-landing-page && npm install",
  "routes": [
    {
      "src": "/assets/(.*)",
      "headers": {
        "cache-control": "max-age=31536000, immutable"
      }
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}